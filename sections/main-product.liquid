  // Attach the event listener to the check button
  checkButton.addEventListener('click', handleCheckButtonClick);

  // Function to handle the button close click
  function handleButtonClose() {
    overlay.style.display = 'none';
  }

  // Attach the event listener to the button close
  buttonClose.addEventListener('click', handleButtonClose);
